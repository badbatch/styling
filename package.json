    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-sort-class-members": "^1.6.0",
    "fs-extra": "^8.1.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lerna": "^3.20.1",
    "lodash": "^4.17.15",
    "markdownlint-cli": "^0.20.0",
