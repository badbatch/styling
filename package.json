    "@repodog/tslint-config": "^0.3.26",
    "@repodog/types": "^0.3.26",
    "@types/enzyme": "^3.10.5",
    "@types/fs-extra": "^8.1.0",
    "@types/jest": "^25.1.3",
    "@types/lodash": "^4.14.149",
    "@types/mock-fs": "^4.10.0",
