{
  "name": "styling",
  "version": "0.0.11",
  "private": true,
  "description": "A css-in-js library that extracts css into stylesheets at build time.",
  "keywords": [
    "css-in-js",
    "react",
    "styled components",
    "stylesheet",
    "styling"
  ],
  "homepage": "https://github.com/dylanaubrey/styling",
  "bugs": {
    "url": "https://github.com/dylanaubrey/styling/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dylanaubrey/styling"
  },
  "license": "MIT",
  "author": "Dylan Aubrey <dylanaubrey@gmail.com>",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "yarn run clean:libs && yarn run compile",
    "clean": "yarn run clean:packages && yarn run clean:libs && yarn run clean:repo",
    "clean:libs": "lerna exec --parallel -- del-cli ./lib",
    "clean:packages": "lerna exec --parallel -- del-cli ./node_modules",
    "clean:repo": "del-cli ./node_modules",
    "compile": "lerna run compile --stream",
    "compile-watch": "lerna run compile-watch --parallel",
    "compile:browser": "lerna run compile:browser --stream",
    "compile:main": "lerna run compile:main --stream",
    "compile:module": "lerna run compile:module --stream",
    "compile:types": "lerna run compile:types --stream",
    "cutoff": "cutoff-lerna",
    "cutoff:post-version": "yarn run compile",
    "init": "yarn && yarn run bootstrap",
    "lint": "tslint --project ./tsconfig.json",
    "new-monorepo": "new-monorepo",
    "new-package": "new-package",
    "new-package:post": "yarn run bootstrap",
    "publish-cutoff": "publish-lerna-cutoff",
    "test": "jest --logHeapUsage",
    "type-check": "tsc --noEmit",
    "validate": "yarn run build && yarn run lint && yarn run type-check && yarn run test",
    "wipe": "yarn run wipe:packages && yarn run wipe:repo",
    "wipe:packages": "lerna exec --parallel -- del-cli ./yarn.lock ./node_modules",
    "wipe:repo": "del-cli ./yarn.lock ./node_modules"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-export-default-from": "^7.7.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.7.4",
    "@babel/plugin-proposal-function-sent": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-numeric-separator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-proposal-throw-expressions": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@repodog/babel-config": "^0.3.28",
    "@repodog/build-references": "^0.3.26",
    "@repodog/commitlint-config": "^0.3.23",
    "@repodog/config-helpers": "^0.3.30",
    "@repodog/constants": "^0.3.28",
    "@repodog/eslint-config": "^0.3.25",
    "@repodog/helpers": "^0.3.25",
    "@repodog/jest-config": "^0.3.29",
    "@repodog/lerna-config": "^0.3.23",
    "@repodog/markdownlint-config": "^0.3.23",
