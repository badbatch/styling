    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.3",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "ts-toolbelt": "^6.1.4",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "type-fest": "^0.8.1",
