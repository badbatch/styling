    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.6.2",
    "ts-toolbelt": "^6.3.5",
    "tslint": "^6.0.0",
