    "@repodog/rollup-config": "^0.3.25",
    "@repodog/ts-config": "^0.3.23",
    "@repodog/tslint-config": "^0.3.24",
    "@repodog/types": "^0.3.25",
    "@types/enzyme": "^3.10.4",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.25",
    "@types/lodash": "^4.14.149",
    "@types/mock-fs": "^4.10.0",
    "@types/node": "^13.1.2",
    "@types/react": "^16.9.16",
    "babel-eslint": "^10.0.3",
